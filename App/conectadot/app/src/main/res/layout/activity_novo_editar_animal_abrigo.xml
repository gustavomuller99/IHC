<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="novoEditarAnimalAbrigoVM" type="com.conectadot.app.modules.novoeditaranimalabrigo.data.viewmodel.NovoEditarAnimalAbrigoVM"/>
    </data>
    <FrameLayout android:id="@+id/frameNovoEditarani"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStyledeep_purple_A100">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_19pxv"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_24pxv"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowup"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowup"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtNovoanimal"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_21pxv"
                        android:layout_marginEnd="@dimen/_130pxh"
                        android:layout_marginStart="@dimen/_81pxh"
                        android:layout_marginTop="@dimen/_22pxv"
                        android:layout_weight="0.78"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{novoEditarAnimalAbrigoVM.novoEditarAnimalAbrigoModel.txtNovoanimal}"
                        style="@style/txtInterbold17" tools:text="@string/lbl_novo_animal"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_black_900_radius_5"
                android:ellipsize="end" android:hint="@string/lbl_nome3"
                android:id="@+id/etGroupThirtySix"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_19pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_14pxh"
                android:paddingTop="@dimen/_19pxv"
                android:singleLine="true"
                android:text="@={novoEditarAnimalAbrigoVM.novoEditarAnimalAbrigoModel.etGroupThirtySixValue}"
                android:textColorHint="@color/black_900"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_black_900_radius_5"
                android:ellipsize="end"
                android:hint="@string/lbl_esp_cie2"
                android:id="@+id/etGroupThirtyFive"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_18pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_20pxv"
                android:singleLine="true"
                android:text="@={novoEditarAnimalAbrigoVM.novoEditarAnimalAbrigoModel.etGroupThirtyFiveValue}"
                android:textColorHint="@color/black_900"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_black_900_radius_5"
                android:ellipsize="end" android:hint="@string/lbl_ra_a3"
                android:id="@+id/etGroupThirtyTwo"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_18pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_20pxv"
                android:singleLine="true"
                android:text="@={novoEditarAnimalAbrigoVM.novoEditarAnimalAbrigoModel.etGroupThirtyTwoValue}"
                android:textColorHint="@color/black_900"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_black_900_radius_5"
                android:ellipsize="end"
                android:hint="@string/lbl_idade2"
                android:id="@+id/etGroupThirtyFour"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_19pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_19pxv"
                android:singleLine="true"
                android:text="@={novoEditarAnimalAbrigoVM.novoEditarAnimalAbrigoModel.etGroupThirtyFourValue}"
                android:textColorHint="@color/black_900"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end" android:id="@+id/txtPorte"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_43pxh"
                android:layout_marginStart="@dimen/_43pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{novoEditarAnimalAbrigoVM.novoEditarAnimalAbrigoModel.txtPorte}"
                style="@style/txtRobotoromanregular14" tools:text="@string/lbl_porte2"/>
            <RadioGroup android:gravity="center_horizontal"
                android:id="@+id/rgRadiogrouppequeno"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <RadioButton android:buttonTint="@color/gray_600"
                    android:ellipsize="end"
                    android:fontFamily="@font/roboto"
                    android:id="@+id/rbPequeno"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:paddingBottom="@dimen/_1pxv"
                    android:paddingStart="@dimen/_8pxh"
                    android:paddingTop="@dimen/_2pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_pequeno2"
                    android:textColor="@color/black_900"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="normal" tools:text="@string/lbl_pequeno2"/>
                <RadioButton android:buttonTint="@color/gray_600"
                    android:ellipsize="end"
                    android:fontFamily="@font/roboto"
                    android:id="@+id/rbMdio"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_width="wrap_content"
                    android:paddingBottom="@dimen/_1pxv"
                    android:paddingStart="@dimen/_10pxh"
                    android:paddingTop="@dimen/_1pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_m_dio2"
                    android:textColor="@color/black_900"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="normal" tools:text="@string/lbl_m_dio2"/>
                <RadioButton android:buttonTint="@color/gray_600"
                    android:ellipsize="end"
                    android:fontFamily="@font/roboto"
                    android:id="@+id/rbGrande"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_width="wrap_content"
                    android:paddingBottom="@dimen/_1pxv"
                    android:paddingStart="@dimen/_10pxh"
                    android:paddingTop="@dimen/_1pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_grande"
                    android:textColor="@color/black_900"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="normal" tools:text="@string/lbl_grande"/>
            </RadioGroup>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_black_900_radius_5"
                android:ellipsize="end"
                android:hint="@string/msg_detalhes_castra"
                android:id="@+id/etGroupThirtyThree"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_18pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_20pxv"
                android:singleLine="true"
                android:text="@={novoEditarAnimalAbrigoVM.novoEditarAnimalAbrigoModel.etGroupThirtyThreeValue}"
                android:textColorHint="@color/black_900"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_black_900_radius_5"
                android:ellipsize="end"
                android:hint="@string/msg_detalhes_vacina"
                android:id="@+id/etGroupThirtyOne"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_18pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_20pxv"
                android:singleLine="true"
                android:text="@={novoEditarAnimalAbrigoVM.novoEditarAnimalAbrigoModel.etGroupThirtyOneValue}"
                android:textColorHint="@color/black_900"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            <LinearLayout android:id="@+id/linearRowgroupthirtyeight"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageButton
                    android:background="@drawable/rectangle_bg_amber_200_radius_32"
                    android:id="@+id/btnGroupThirtyEight"
                    android:layout_height="@dimen/_64pxh"
                    android:layout_width="@dimen/_64pxh"
                    android:paddingHorizontal="@dimen/_17pxh"
                    android:paddingVertical="@dimen/_17pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_group38" tools:ignore="ContentDescription"/>
                <ImageButton
                    android:background="@drawable/rectangle_bg_amber_200_radius_32"
                    android:id="@+id/btnGroupThirtySeven"
                    android:layout_height="@dimen/_64pxh"
                    android:layout_marginStart="@dimen/_172pxh"
                    android:layout_width="@dimen/_64pxh"
                    android:paddingHorizontal="@dimen/_17pxh"
                    android:paddingVertical="@dimen/_17pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_group37" tools:ignore="ContentDescription"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
